<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	.titel{
		font-family: times new roman, times, serif;
		font-size: 16px;
		font-weight: bold;
}
	.area{
		font-family: times new roman, times, serif;
		margin-top: 10px;
}
	</ui:style>

	<m:MaterialCollection >
	
		<m:MaterialCollectionItem type="AVATAR" ui:field="collectionItemUi">
 			<m:MaterialLabel ui:field="titelUi" text="Inconnu" styleName="{style.titel}" />
 			<m:MaterialImage ui:field="imageUi" url="img/a_user.jpg" type="CIRCLE" marginTop="-20" />
 			<m:MaterialTextArea ui:field="textAreaUi" enabled="false" styleName="{style.area}"/>
 		


 		</m:MaterialCollectionItem>

	</m:MaterialCollection>

</ui:UiBinder> 