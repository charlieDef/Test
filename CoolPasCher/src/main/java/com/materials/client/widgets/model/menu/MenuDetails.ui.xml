<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:fu='urn:import:com.materials.client.widgets.upload' xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mdf='urn:import:com.materials.client.widgets.form'
	xmlns:ma='urn:import:gwt.material.design.addins.client'>
	<ui:style>
	
	.cardTitle {
		color: #ffb140;
		margin-top: 1px;
		font-family: times new roman, times, serif;
		font-size: 20px !important;
			text-shadow: 1px 1px 1px black;
	}
	
	.cardDescription{
	color: rgba(0, 0, 0, 0.87);
}
	
	.cardContent {
		padding-top: 1px !important;
		font-family: times new roman, times, serif;
		background: white;
	}
	
	</ui:style>
	<mdf:MDFormPanel ui:field="mdFormUi">


		<mdf:childheader1>
		<m:MaterialIcon ui:field="backButtonUi" waves="LIGHT" marginLeft="-20"
				iconType="KEYBOARD_BACKSPACE" iconPosition="LEFT" iconFontSize="40" />
		</mdf:childheader1>

		<mdf:childheader2>
			<m:MaterialLabel ui:field="textHeaderLabelUi" text="Details" />
		</mdf:childheader2>


		<mdf:child bindName="name">
			<m:MaterialTextBox ui:field="nameUi" label="Nom" enabled="false"/>
		</mdf:child>
		
				<mdf:child bindName="description">
			<m:MaterialTextBox ui:field="descriptionUi" label="Description" enabled="false"/>
		</mdf:child>
		
		<mdf:child bindName="index">
			<m:MaterialIntegerBox ui:field="indexUi" label="Index" enabled="false"/>
		</mdf:child>
	
		<mdf:child bindName="type">
			<ma:combobox.MaterialComboBox ui:field="typeListUi" label="Type" enabled="false"/>
		</mdf:child>
		

		<mdf:child bindName="">
			<ma:combobox.MaterialComboBox ui:field="categoryListUi" label="Category" enabled="false"/>
		</mdf:child>

		<mdf:child bindName="">
			<ma:combobox.MaterialComboBox ui:field="parentMenuListBoxUi"
				label="ParentMenu" enabled="false"/>
		</mdf:child>
		
			<mdf:child bindName="textInfo">
			<m:MaterialTextBox ui:field="textInfoUi" label="Text Info" enabled="false"/>
		</mdf:child>

	<mdf:child bindName="ddAlignRight">
			<m:MaterialCheckBox ui:field="ddAlignRightUi" value="false"
				type="FILLED" text="DropDown Right" enabled="false"/>
		</mdf:child>

	<mdf:child bindName="hasContent">
			<m:MaterialCheckBox ui:field="hasContentUi" value="false"
				type="FILLED" text="Contents" enabled="false"/>
		</mdf:child>
		
		<mdf:child bindName="memberOnly">
			<m:MaterialCheckBox ui:field="memberOnlyUi" value="false"
				type="FILLED" text="MemberOnly" enabled="false"/>
		</mdf:child>
		
		<mdf:child bindName="menuCategory">
			<m:MaterialCheckBox ui:field="menuCategoryUi" value="false"
				type="FILLED" text="MenuCategory" enabled="false"/>
		</mdf:child>

		<mdf:child bindName="active">
			<m:MaterialCheckBox ui:field="actifUi" value="false"
				type="FILLED" text="Actif" enabled="false"/>
		</mdf:child>

		<mdf:child bindName="lock">
			<m:MaterialCheckBox ui:field="lockUi" visible="false"
				value="false" type="FILLED" enabled="false"/>
		</mdf:child>

	</mdf:MDFormPanel>

</ui:UiBinder> 