<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:fu='urn:import:com.materials.client.widgets.upload' xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mdf='urn:import:com.materials.client.widgets.form'
	xmlns:ma='urn:import:gwt.material.design.addins.client' xmlns:ie="urn:import:com.materials.client.widgets.upload.image">
	<ui:style>
	
	.container {
		font-family: times new roman, times, serif;
		text-align: left;
		padding:20px;
		overflow: auto;
		margin: 10px;
	}
	
		.middleArea {

	margin-bottom: 10px;
	border-bottom: 1px solid #ccc;


	}
	
	.columnBottomRight{
		padding-left: 20px;
		border-left: 1px solid #eee ;
	}
	
	</ui:style>
	
	
	<m:MaterialCard ui:field="materialCardUi" styleName="{style.container}">
		
		<m:MaterialRow  ui:field="materialRow1Ui" styleName="" >
			<m:MaterialColumn ui:field="columnTopLeft" styleName=""></m:MaterialColumn>
			<m:MaterialPanel ui:field="columnTopRight" styleName="" textAlign="CENTER"></m:MaterialPanel>
		</m:MaterialRow>
		
		<m:MaterialRow  ui:field="materialRow2Ui" styleName=""/>
		
		<m:MaterialRow  ui:field="materialRowSliderUi" styleName="{style.middleArea}"/>
		
		<m:MaterialRow  ui:field="materialRow4Ui" styleName="">
			<m:MaterialColumn grid="s12 l7"  ui:field="columnBottomLeft" styleName=""></m:MaterialColumn>
			<m:MaterialColumn  grid="s12 l5" ui:field="columnBottomRight" styleName="{style.columnBottomRight}" ></m:MaterialColumn>
		</m:MaterialRow>

	</m:MaterialCard>
	
	
</ui:UiBinder> 