<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:fu='urn:import:com.materials.client.widgets.upload' xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mdf='urn:import:com.materials.client.widgets.form'
	xmlns:ma='urn:import:gwt.material.design.addins.client' xmlns:ie="urn:import:com.materials.client.widgets.upload.image">
	<ui:style>
	
	.container {
		font-family: times new roman, times, serif;
		text-align: left;
		padding:20px;
		overflow: auto;
		margin: 10px;
	}
	
	.middleArea {
		margin-bottom: 10px;
	}
	
	.title{
		font-weight: bold;
	}
	
	.columnBottomRight{
		padding-left: 20px;
		border-left: 1px solid #eee ;
	
		overflow: auto;
	}
	
		.columnBottomRight{
		padding-left: 20px;
		border-left: 1px solid #eee ;
	
		overflow: auto;
	}
	
	

	
		.materialRowSliderRight{
		padding-left: 20px;
	
		overflow: auto;
		margin-left: 0px;
	}
	



	
	</ui:style>
	
	
	<m:MaterialCard ui:field="materialCardUi" styleName="{style.container}">
		
		<!-- Title Area -->
		<m:MaterialRow  ui:field="materialRow1Ui" styleName="" >
			<m:MaterialPanel ui:field="columnTop" styleName="" textAlign="LEFT">
				<m:MaterialLabel ui:field="labelUi" styleName="{style.title}" />
			</m:MaterialPanel>

			
		</m:MaterialRow>
			
		<m:MaterialRow  styleName="{style.middleArea}">
				<!-- Left Area -->
				<m:MaterialColumn  ui:field="materialColumnSliderLeftUi" marginBottom="20" styleName=""    grid="s12 l8" >
				
				</m:MaterialColumn>	
				
				<!-- RIGHT Area -->
				<m:MaterialColumn  ui:field="materialColumnSliderRightUi" styleName="{style.materialRowSliderRight}"   grid="s12 l4">
				
					<!-- Auteur Area -->
					<m:MaterialPanel ui:field="panelOwnerUi" marginBottom="60" />

					<!-- Commentaires Area -->		
					<m:MaterialPanel ui:field="panelCommentUi"    />
				
				</m:MaterialColumn>
		</m:MaterialRow>
		
		<m:MaterialRow  ui:field="materialRow4Ui" styleName="">
			<m:MaterialColumn grid="s12 l7"  ui:field="columnBottomLeft" styleName=""></m:MaterialColumn>
			<m:MaterialColumn  grid="s12 l5" ui:field="columnBottomRight" styleName="{style.columnBottomRight}" ></m:MaterialColumn>
		</m:MaterialRow>

	</m:MaterialCard>
	
	
</ui:UiBinder> 