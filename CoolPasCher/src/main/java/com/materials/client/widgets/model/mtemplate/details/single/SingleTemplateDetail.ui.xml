<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:fu='urn:import:com.materials.client.widgets.upload' xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mdf='urn:import:com.materials.client.widgets.form'
	xmlns:ma='urn:import:gwt.material.design.addins.client'>
	<ui:style>
	.card{
	
	border: 1px solid #696969;
	padding: 10px;
}

		.container {
	
		text-align: center !important;
		margin-left:20px;
			z-index:15;
	}
	.test{

text-align: right !important;
}
	.card2{
	 	z-index:15;
}


		
	</ui:style>
	<mdf:MDFormPanel ui:field="mdFormUi">

		<mdf:childheader1>
			<m:MaterialIcon ui:field="backButtonUi" waves="LIGHT" marginLeft="-20"
				iconType="KEYBOARD_BACKSPACE" iconPosition="LEFT" iconFontSize="40" />
		</mdf:childheader1>

		<mdf:childheader2>
			<m:MaterialLabel ui:field="textHeaderLabelUi" text="Details" />
		</mdf:childheader2>
		
		<mdf:childtop>

<g:HTMLPanel ui:field="templateUi"  styleName="{style.container}"/>

		</mdf:childtop>
		
		<mdf:child bindName="upload">
			<fu:MDFileUploader title="420x225" visible="false"
				ui:field="fileUploadUi" />
		</mdf:child>

		<mdf:child bindName="category">
			<m:MaterialTextBox ui:field="categoryUi" label="Category" enabled="false"/>
		</mdf:child>
		
				<mdf:child bindName="name">
			<m:MaterialTextBox ui:field="nameUi" label="Name" enabled="false"/>
		</mdf:child>
		
			<mdf:child bindName="linkName">
			<m:MaterialTextBox ui:field="liensUi" label="Liens" enabled="false"/>
		</mdf:child>
		
			<mdf:child bindName="index">
			<m:MaterialIntegerBox ui:field="indexUi" label="Index" enabled="false"/>
		</mdf:child>
		
			<mdf:child bindName="buttonStyle">
			<m:MaterialCheckBox ui:field="buttonStyleUi" value="false"
				type="FILLED" text="ButtonStyle" enabled="false"/>
		</mdf:child>
		
					<mdf:child bindName="leftAlign">
			<m:MaterialCheckBox ui:field="leftAlignUi" value="false"
				type="FILLED" text="LeftAlign" enabled="false"/>
		</mdf:child>
		
		<mdf:child bindName="active">
			<m:MaterialCheckBox ui:field="activeUi" value="false"
				type="FILLED" text="Actif" enabled="false"/>
		</mdf:child>
		
			<mdf:child bindName="lock">
			<m:MaterialCheckBox ui:field="lockUi" value="false"
				type="FILLED" text="Lock" enabled="false"/>
		</mdf:child>

	</mdf:MDFormPanel>

</ui:UiBinder> 