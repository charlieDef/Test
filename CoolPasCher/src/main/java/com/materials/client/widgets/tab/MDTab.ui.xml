<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client" xmlns:mh="urn:import:com.materials.client.views">
	<ui:style>
	
		.content{
			color: #ffb74d !important;
background: #8b0000 !important;

		}
	
		.topArea{
			background: none repeat scroll 0 0 #8b0000 !important;
			border-bottom: 1px solid #ffb74d !important;
			color: #ffb74d !important;
				font-family: times new roman, times, serif;
		}
		
		.def {
text-align: center;
		}
	
		.titel {
		font-weight: bold;
		font-size: 20px;
			font-family: times new roman, times, serif;
		
	}
	
	.content2{
	border-radius: 5px;
}
	
	</ui:style>
		<m:MaterialRow ui:field="materialRowUi"  >
	
		<m:MaterialColumn grid="s12 m12 l10">
	<m:MaterialCard   marginRight="10" marginTop="0" styleName="{style.content2}" >
	<m:MaterialCardContent padding="10">
		<m:MaterialRow ui:field="tuu" styleName="{style.topArea}"  margin="0" >
		
			<m:MaterialRow>
				<m:MaterialColumn grid="s1 m1 l1"  ui:field="iconUi" visible="false"/>
				<m:MaterialColumn ui:field="titelAreaUi" grid="s11 m11 l10" textAlign="CENTER" >
					<m:MaterialCardTitle ui:field="cardTitelUi" fontSize="1.5em" text="Nouvelle annonce" styleName="{style.def}" textAlign="CENTER" />
				</m:MaterialColumn>
			</m:MaterialRow>

			
		
			
			<m:MaterialRow ui:field="headerAreaUi" styleName="{style.titel}" />
			<m:MaterialRow ui:field="headerAreaUi2" styleName="{style.titel}"/>
		</m:MaterialRow>
		
		<m:MaterialRow ui:field="contentAreaUi"  >
			<m:MaterialTab ui:field="tabUi"  shadow="1" backgroundColor="WHITE" indicatorColor="AMBER_DARKEN_3" styleName="{style.content}"/>
		</m:MaterialRow>
				</m:MaterialCardContent>
	</m:MaterialCard>
	
		</m:MaterialColumn>
	
		</m:MaterialRow>
</ui:UiBinder> 