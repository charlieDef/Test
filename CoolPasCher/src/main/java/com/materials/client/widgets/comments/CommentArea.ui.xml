		</m:MaterialCollapsibleHeader>
 		<m:MaterialCollapsibleBody >
 			<m:MaterialPanel>
 			<m:MaterialRow ui:field="commentRowUi"/>
 			
 			<m:MaterialRow>
 			<m:MaterialCollection ui:field="commentPanelUi"/>
 			</m:MaterialRow>
 			
 			</m:MaterialPanel>
 
 					
 		</m:MaterialCollapsibleBody>
 </m:MaterialCollapsibleItem>

	</m:MaterialCollapsible>

</ui:UiBinder> 